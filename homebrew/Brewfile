# homebrew basics
tap "homebrew/dupes"
tap "homebrew/versions"
brew "homebrew/dupes/grep"

# useful utilities
brew "grc"
brew "coreutils"
brew "spark"
brew "wget"
brew "pwgen"

# Install other useful binaries
brew "ack"
brew "git"
brew "node"
brew "tree"
brew "vim", args: ['override-system-vi']
brew "dnsmasq"

# brew go
brew "go", args: ['cross-compile-common'] 

